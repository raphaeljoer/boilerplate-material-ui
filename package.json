{
  "name": "boilerplate-material-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start NODE_ENV=production",
    "dev": "react-scripts start NODE_ENV=development",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src --max-warnings=0",
    "test": "react-scripts test NODE_ENV=test",
    "test:watch": "react-scripts test --watch NODE_ENV=test"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.2",
    "@material-ui/core": "^5.0.0-beta.5",
    "@mui/lab": "^5.0.0-alpha.51",
    "@mui/material": "^5.0.1",
    "@reduxjs/toolkit": "^1.6.2",
    "date-fns": "^2.25.0",
    "i18next": "^21.3.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.5",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/i18next": "^13.0.0",
    "@types/i18next-browser-languagedetector": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "prettier": "2.4.1",
    "react-beautiful-dnd": "^13.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
